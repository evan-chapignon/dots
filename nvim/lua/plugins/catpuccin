return {
    {
        "catppuccin/nvim",
        lazy = false,
        name = "catppuccin",
        priority = 1000,
        config = function()
            local hyprland = os.getenv("HYPRLAND_INSTANCE_SIGNATURE")
            if hyprland then
                vim.cmd.colorscheme("gruvbox")
            else
                vim.cmd.colorscheme("gruvbox")
            end
        end,
    },
}
